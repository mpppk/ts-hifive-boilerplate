{
  "name": "ts-hifive-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "test": "jest",
    "doc": "typedoc --out ./docs -m \"amd\"",
    "start": "webpack-dev-server",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "keywords": [],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.2.13",
    "@types/mocha": "^2.2.41",
    "@types/node": "^10.0.3",
    "ejs-compiled-loader": "^1.1.0",
    "html-webpack-plugin": "^3.0.0",
    "jest": "^23.5.0",
    "source-map-support": "^0.5.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^3.0.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.11.0",
    "typescript": "^2.4.2",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "codacy-coverage": "^3.0.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
