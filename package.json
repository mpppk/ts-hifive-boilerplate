{
  "name": "ts-hifive-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "test": "nyc mocha",
    "doc": "typedoc --out ./docs -m \"amd\"",
    "start": "webpack-dev-server --inline",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "codacycov": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^3.2.13",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.34",
    "@types/power-assert": "^1.4.29",
    "chai": "^4.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "ejs-compiled-loader": "^1.1.0",
    "espower-typescript": "^8.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "isparta": "^4.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "power-assert": "^1.4.4",
    "source-map-support": "^0.4.15",
    "ts-loader": "^2.3.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "codacy-coverage": "^2.0.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
